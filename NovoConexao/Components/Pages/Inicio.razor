@page "/"
@using NovoConexao.Models


<video autoplay muted loop class="bgVideo">
    <source src="/images/bayer-video.mp4" type="video/mp4">
    Seu navegador não suporta vídeos HTML5.
</video>

<div class="widget-container">
    <img src="/images/LogoBayer.png" style="height: calc(3em + 2.4em); float: left; margin-right: 20px; margin-top: 10px;">
    <div style="overflow: hidden; margin-left: 10px;">
        <h1 style="font-family: 'Segoe UI', Arial, sans-serif; font-size: 2.5em; color: #FFFFFF; margin-bottom: 10px; margin-top: 0; text-align: left;">Conexão OE&PI</h1>
        <h3 style="font-family: 'Segoe UI', Arial, sans-serif; font-size: 1.5em; font-weight: lighter; color: #FFFFFF; margin-bottom: 40px; margin-top: 0; text-align: left;">Produção de Soja e Algodão Latam</h3>
    </div>

    <!-- Grupos de Registros Negocio -->
    <div class="text-container">
        <span class="text-left">/////</span>
        <span class="text-right">Negócios</span>
    </div>
    <div class="grid-main" style="margin-bottom:40px">
        @foreach (var negocio in listaNegocio)
        {
            <div class="widget-button-negocio">
                <button type="button" onclick="window.location.href = '@negocio.UrlText'" class="widget-button-negocio">
                    <div class="div-imagem">
                        <img src="@($"{negocio.IconNegocio}")" alt="@negocio.NameNegocio">
                    </div>
                </button>
            </div>
        }
    </div>

</div>
<!-- Grupos de Registros Links -->

<div class="text-container">
    <span class="text-left">/////</span>
    <span class="text-right">Links Úteis</span>
</div>
<div class="grid-main-links">
    @foreach (var link in listaLink.Where(l => l.Categoria == "Link"))
    {
        <div class="widget-button1x1">
            <button type="button" onclick="window.open('@link.LinkAcesso', '_blank')" class="widget-button1x1">
                <svg width="24" height="24" fill="none" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path d="M6.25 4.75a1.5 1.5 0 0 0-1.5 1.5v11.5a1.5 1.5 0 0 0 1.5 1.5h11.5a1.5 1.5 0 0 0 1.5-1.5v-4a1 1 0 1 1 2 0v4a3.5 3.5 0 0 1-3.5 3.5H6.25a3.5 3.5 0 0 1-3.5-3.5V6.25a3.5 3.5 0 0 1 3.5-3.5h4a1 1 0 1 1 0 2h-4Zm6.5-1a1 1 0 0 1 1-1h6.5a1 1 0 0 1 1 1v6.5a1 1 0 1 1-2 0V6.164l-4.793 4.793a1 1 0 1 1-1.414-1.414l4.793-4.793H13.75a1 1 0 0 1-1-1Z" fill="#ffffff" /></svg>
                <div>@link.Nome</div>
            </button>

        </div>


    }
</div>

<div class="footer-text">
    <h3>Desenvolvido por OE&PI</h3>
</div>

<div class="footer-text-left">
    <h3>Não achou o link que precisa? Nos informe <a href="https://forms.office.com/e/9TbiYDY27n" target="_blank" style="color: white; text-decoration: underline;">aqui</a></h3>
</div>




